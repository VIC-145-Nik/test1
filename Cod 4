#include <iostream>
#include <algorithm>

int main(int argc, char* argv[])
{
int a, b;
std::cout « "Введите диапазон (a : b):" « std::endl;
std::cin » a » b;
if (a > b)
std::swap(a, b);
for (int i = a; i <= b; i++)
std::cout « i « " ";
std::cout « std::endl;
char symbol;
std::cout « "Введите символ:" « std::endl;
std::cin » symbol;
int n;
std::cout « "Введите колличество символов:" « std::endl;
std::cin » n;
for (int i = 0; i < n; i++)
std::cout « symbol;
std::cout « std::endl;

